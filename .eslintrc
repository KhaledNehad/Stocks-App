{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["@react-native-community", "plugin:prettier/recommended"],
  "rules": {
    "indent": ["warn", 2],
    "quotes": ["error", "single"],
    "no-console": "warn",
    "semi": "off"
  },
  "env": {
    "jest": true
  }
}
